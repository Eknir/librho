new abs in {
  contract abs(@{input /\ Int}, return) = {
    if (input >= 0) {
      return!(input)
    }
    else {
      return!(-input)
    }
  } |

  contract abs(@{{String} \/ {Bool} \/ {ByteArray} \/ {Uri}}, return) = {
    return!("No integer provided")
  }  |

  // Unit tests below
  new stdout(`rho:io:stdout`) in {
    abs!(5, *stdout)|
    abs!(0, *stdout)|
    abs!(-5, *stdout)|
    abs!("123", *stdout) |
    abs!(true, *stdout) 
  }
}





